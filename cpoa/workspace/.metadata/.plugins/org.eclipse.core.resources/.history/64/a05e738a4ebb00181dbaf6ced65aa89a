package math;

public class Addition
{
	public Addition() {
		super();
	}

	public Long calculer(Long a,Long b){
		return a+b;
	}
		
	public Character lireSymbole()
	{
		return '-';
	}

	/**
	 * @param args
	 */
	public static void main(String[] args)
	{
		Addition op = new Addition();	
		Long resultat = op.calculer(new Long(1),new Long(3));
		System.out.println("L'addition de 6 et de 3 donne " + resultat.toString());

	}

}
